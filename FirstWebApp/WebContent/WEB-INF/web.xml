<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
	<welcome-file-list>
        <welcome-file>/WEB-INF/jsp/login.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>FirstServlet</servlet-name>
        <servlet-class>com.pnb.first.FirstServlet</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>FirstServlet</servlet-name>
        <url-pattern>/processServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>SecondServlet</servlet-name>
        <servlet-class>com.pnb.first.SecondServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SecondServlet</servlet-name>
        <url-pattern>/processSecondServlet</url-pattern>
    </servlet-mapping>
</web-app>